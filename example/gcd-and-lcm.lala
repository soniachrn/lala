
| Euclidean algorithm
function gcd(var a: int, var b: int): int {
    while b != 0 {
        var t: int = b
        b = a % b
        a = t
    }
    return a
}

function lcm(var a: int, var b: int): int
    return a * b / gcd(a, b)


var a: int = 18
var b: int = 12

print 'GCD(' + a: string + ', ' + b: string + ') = ' + gcd(a, b): string
print 'LCM(' + a: string + ', ' + b: string + ') = ' + lcm(a, b): string

